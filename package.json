{
  "name": "react-starter",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "start": "ESLINT_NO_DEV_ERRORS='true' react-scripts start",
    "build": "react-scripts build",
    "generate:svg-sprite": "svg2sprite ./src/assets/icons ./src/assets/sprites/_sprite.svg --stripAttrs id",
    "test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "lint:style": "stylelint --fix \"src/**/*.scss\" --syntax=scss -q",
    "prettier": "npx prettier --write .",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "antd": "5.10.2",
    "normalize.css": "8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-helmet-async": "1.3.0",
    "react-redux": "8.1.3",
    "react-router-dom": "6.17.0",
    "react-scripts": "5.0.1",
    "reselect": "4.1.8"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@jedmao/redux-mock-store": "3.0.5",
    "@reduxjs/toolkit": "1.9.7",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.0",
    "@testing-library/user-event": "14.5.1",
    "@types/jest": "29.5.6",
    "@types/node": "20.8.8",
    "@types/react": "18.2.31",
    "@types/react-dom": "18.2.14",
    "@types/react-helmet": "6.1.8",
    "@types/react-redux": "7.1.28",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "eslint": "8.52.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "lint-staged": "15.0.2",
    "postcss": "8.4.31",
    "prettier": "2.6.2",
    "sass": "1.69.4",
    "stylelint": "13.6.1",
    "stylelint-declaration-use-variable": "1.7.3",
    "stylelint-order": "5.0.0",
    "stylelint-scss": "4.2.0",
    "svg2sprite-cli": "2.0.1",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "*.{tsx,ts}": "eslint --fix",
    "*.scss": "stylelint --fix \"src/**/*.scss\" --syntax=scss -q"
  }
}
